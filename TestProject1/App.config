    <sources>
      <source name="System.Windows.Data" switchName="mySwitch">
        <listeners>
          <add name="myListener" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="mySwitch" value="All" />
    </switches>
    <sharedListeners>
      <add name="myListener" type="System.Diagnostics.TextWriterTraceListener"
